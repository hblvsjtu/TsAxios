{
    "name": "tspaxios",
    "version": "0.0.7",
    "description": "axios with typeScript",
    "keywords": [
        "axios",
        "ts"
    ],
    "main": "dist/axios.umd.js",
    "module": "dist/axios.es5.js",
    "typings": "dist/types/axios.d.ts",
    "files": [
        "dist",
        "src/types",
        "README.md",
        "CHANGELOG.md",
        "docs"
    ],
    "author": "hblvsjtu <hblvsjtu@163.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/hblvsjtu/TsAxios.git"
    },
    "license": "MIT",
    "engines": {
        "node": ">=6.0.0"
    },
    "scripts": {
        "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
        "prebuild": "rimraf dist",
        "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
        "start": "rollup -c rollup.config.ts -w",
        "test": "cross-env NODE_ENV=test && jest --coverage",
        "pretty": "pretty-quick",
        "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md docs && git commit --amend --no-edit",
        "commit": "git-cz",
        "precommit": "lint-staged",
        "test:watch": "jest --coverage --watch",
        "test:prod": "npm run lint && npm run test -- --no-cache",
        "deploy-docs": "ts-node tools/gh-pages-publish",
        "report-coverage": "cat ./coverage/lcov.info | coveralls",
        "semantic-release": "semantic-release",
        "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
        "travis-deploy-once": "travis-deploy-once"
    },
    "dependencies": {
        "@types/lodash": "^4.14.157",
        "lodash": "^4.17.15",
        "xmlhttprequest": "^1.8.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.1.2",
        "@commitlint/config-conventional": "^7.1.2",
        "@types/jest": "^23.3.2",
        "@types/node": "^10.11.0",
        "body-parse": "^0.1.0",
        "colors": "^1.3.2",
        "commitizen": "^3.0.0",
        "conventional-changelog-cli": "^2.0.34",
        "coveralls": "^3.0.2",
        "cross-env": "^5.2.0",
        "cz-conventional-changelog": "^2.1.0",
        "express": "^4.17.1",
        "husky": "^1.0.1",
        "jest": "^23.6.0",
        "jest-config": "^23.6.0",
        "lint-staged": "^8.0.0",
        "lodash.camelcase": "^4.3.0",
        "prettier": "^1.14.3",
        "pretty-quick": "^2.0.1",
        "prompt": "^1.0.0",
        "replace-in-file": "^3.4.2",
        "rimraf": "^2.6.2",
        "rollup": "^0.67.0",
        "rollup-plugin-commonjs": "^9.1.8",
        "rollup-plugin-json": "^3.1.0",
        "rollup-plugin-node-resolve": "^3.4.0",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "rollup-plugin-typescript2": "^0.18.0",
        "semantic-release": "^17.2.3",
        "shelljs": "^0.8.3",
        "travis-deploy-once": "^5.0.9",
        "ts-jest": "^23.10.2",
        "ts-loader": "^7.0.5",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "tslint-config-standard": "^8.0.1",
        "tslint-loader": "^3.5.4",
        "typedoc": "^0.12.0",
        "typescript": "^3.0.3"
    },
    "lint-staged": {
        "{src,test}/**/*.ts": [
            "prettier --write",
            "git add"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "maxHeaderWidth": 100,
            "maxLineWidth": 100,
            "defaultType": "",
            "defaultScope": "",
            "defaultSubject": "",
            "defaultBody": "",
            "defaultIssues": "",
            "types": {
                "feat": {
                    "description": "A new feature",
                    "title": "Features"
                },
                "fix": {
                    "description": "A new fix",
                    "title": "Features"
                },
                "docs": {
                    "description": "A new docs",
                    "title": "Features"
                },
                "style": {
                    "description": "A new style",
                    "title": "Features"
                },
                "refactor": {
                    "description": "A new refactor",
                    "title": "refactor"
                },
                "test": {
                    "description": "A new test",
                    "title": "test"
                },
                "revert": {
                    "description": "A new revert",
                    "title": "revert"
                },
                "config": {
                    "description": "A new config",
                    "title": "config"
                },
                "chore": {
                    "description": "A new chore",
                    "title": "chore"
                }
            }
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && pretty-quick --staged",
            "pre-push": "npm run test"
        }
    }
}
